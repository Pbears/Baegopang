<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper 
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<!-- ORDERNUMBER, MEMBERID, MEMBERNAME, MEMBERTEL, MEMBERADDRESS, PRICE, ORDERINFO, MENUNAME, AMOUNT, STORENAME, 
STOREADDRESS, STORETEL, ORDERTIME, STATE, HIT -->

<mapper namespace="gopang.xml.order"> 
	<insert id="orderInsert" parameterType="foodOrder">
		INSERT INTO FOODORDER VALUES(#{ordernumber},#{memberid},#{membername},#{membertel},#{memberaddress},
														#{price},#{orderinfo},#{menuname},#{amount},#{storename},
														#{storeaddress},#{storetel},#{ordertime},0,0)
	</insert>
  	
</mapper>





