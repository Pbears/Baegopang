<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE sqlMap      
    PUBLIC "-//ibatis.apache.org//DTD SQL Map 2.0//EN"      
    "http://ibatis.apache.org/dtd/sql-map-2.dtd">
    
<sqlMap namespace="member">
	<typeAlias alias="member" type="gopang.vo.memberVo"/>
	<select id="idCheck" parameterClass="java.lang.String" resultClass="java.lang.String" >
		SELECT ID FROM MEMBER WHERE ID=#userid#
	</select>
	<select id="passwordCheck" parameterClass="java.lang.String" resultClass="java.lang.String">
		SELECT PW FROM MEMBER WHERE ID=#userid#
	</select>
</sqlMap>