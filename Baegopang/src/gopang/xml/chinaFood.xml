<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper 
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="gopang.xml.chinaFood"> 
  	<select id="selectChinaFood" resultType="brand" parameterType="java.lang.Integer">
  		SELECT BRANDNO, BRANDNAME, PICTURE
  		FROM BRAND
  		WHERE BRANDNO LIKE #{brandNo}||'%'
  	</select>
  	<select id="getChinaFoodtotalRow" resultType="java.lang.Integer" parameterType="java.lang.Integer">
  		SELECT COUNT(*) FROM BRAND
  		WHERE BRANDNO LIKE #{brandNo}||'%'
  	</select>
</mapper>





